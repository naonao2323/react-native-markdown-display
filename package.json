  "author": "Mient-jan Stelling and Tom Pickard + others from the community",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeremybarbet/react-native-markdown-display/issues"
  },
  "homepage": "https://github.com/jeremybarbet/react-native-markdown-display/",
  "dependencies": {
    "css-to-react-native": "^3.0.0",
