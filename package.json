    "react-native": ">=0.50.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^0.0.7",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "babel-jest": "^29.7.0",
    "eslint": "^6.8.0",
    "fast-check": "^4.5.3",
    "jest": "^29.7.0",
    "pre-commit": "1.2.2",
    "react-test-renderer": "^19.2.4",
    "typescript": "^5.9.3"
  }
